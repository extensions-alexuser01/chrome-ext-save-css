{
  "name": "Save CSS",
  "version": "1.0.2",
  "description": "Save your CSS and JS modifications in Devtools automatically to local disk.",
  "manifest_version": 2,
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "icons": { "16": "icon16.png",
             "128": "icon128.png" },
  "devtools_page": "devtools.html",
  "background": { "scripts" : ["bg.js"]},
  "minimum_chrome_version": "18"
}

